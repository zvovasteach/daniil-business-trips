{
  "name": "daniil-business-trips",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "gv": "genversion autogenerated/version.ts --esm -s",
    "dev": "npm run gv && vite",
    "type-check": "vue-tsc  --noEmit -p tsconfig.app.json",
    "type-check-tests": "vue-tsc  --noEmit -p tsconfig.vitest.json",
    "build": "npm run gv && vite build",
    "preview": "vite preview",
    "stylelint": "stylelint \"src/**/*.{css,scss,sass,vue}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,sass,vue}\" --fix",
    "eslint": "eslint \"src/**/*.{js,ts,vue}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{js,ts,vue}\"",
    "lint": "npm-run-all --parallel --continue-on-error stylelint eslint",
    "lint:fix": "npm-run-all --parallel --continue-on-error stylelint:fix eslint:fix",
    "oxlint": "npx oxlint --type-aware --fix && npx eslint",
    "test:use-node-env": "dotenvx run -f .env.test --",
    "vitest": "vitest",
    "vitest:ui": "vitest --ui",
    "vitest:once": "vitest run",
    "vitest:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "pinia": "^3.0.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "zod": "^4.1.13",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@faker-js/faker": "9.9.0",
    "@msw/playwright": "0.4.2",
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.57.0",
    "@stylistic/eslint-plugin": "5.4.0",
    "@stylistic/stylelint-config": "3.0.1",
    "@stylistic/stylelint-plugin": "4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.10.1",
    "@types/qs": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.3.18",
    "@vitest/ui": "3.2.4",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "browserslist": "^4.28.0",
    "cross-env": "10.1.0",
    "cssnano": "^7.1.2",
    "eslint": "9.37.0",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-oxlint": "1.29.0",
    "eslint-plugin-prefer-arrow-functions": "3.9.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-testing-library": "7.13.2",
    "eslint-plugin-vue": "10.5.0",
    "genversion": "^3.2.0",
    "globals": "^16.5.0",
    "jiti": "2.6.1",
    "jsdom": "27.0.0",
    "msw": "2.11.5",
    "npm-run-all2": "8.0.4",
    "oxlint": "1.29.0",
    "oxlint-tsgolint": "0.7.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.4.0",
    "qs": "^6.14.0",
    "remove-attr": "0.0.15",
    "rollup-plugin-visualizer": "6.0.4",
    "sass": "1.93.2",
